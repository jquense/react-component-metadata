    "mocha": "~1.21.4",
    "react": "^0.12.2",
    "release-script": "^0.5.3",
    "rimraf": "^2.2.8",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.6.0"
  },
  "dependencies": {
    "babel-traverse": "^6.7.4",
    "babel-types": "^6.7.2",
