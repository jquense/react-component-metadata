{
  "name": "react-component-metadata",
  "version": "3.1.0",
  "description": "parse react components for prop data and descriptions",
  "author": {
    "name": "Jason Quense",
    "email": "monastic.panic@gmail.com"
  },
  "repository": "jquense/react-component-metadata",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
